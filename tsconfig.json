{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "paths": {
      "@components/*": [
        "./src/components/*"
      ],
      "@constants": [
        "./src/constants"
      ],
      "@hooks": [
        "./src/hooks"
      ],
      "@navigators": [
        "./src/navigators"
      ],
      "@screens": [
        "./src/screens"
      ],
      "@content/*": [
        "./src/content/*"
      ],
      "@types": [
        "./src/types"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      "@utils": [
        "./src/utils"
      ],
      "@store/*": [
        "./src/store/*"
      ],
      "@store": [
        "./src/store"
      ]
    }
  },
}